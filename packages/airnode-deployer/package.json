    "@types/adm-zip": "^0.5.5",
    "@types/aws-lambda": "^8.10.145",
    "@types/lodash": "^4.17.12",
    "@types/node": "^18.19.57",
    "@types/yargs": "^17.0.33",
    "aws-sdk-client-mock": "^3.1.0",
    "aws-sdk-client-mock-jest": "^3.1.0",
    "copyfiles": "^2.4.1",
    "esbuild-loader": "^4.2.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
