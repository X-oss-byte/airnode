    "@api3/airnode-validator": "^0.12.0",
    "@api3/promise-utils": "^0.4.0",
    "@aws-sdk/client-s3": "^3.420.0",
    "@aws-sdk/signature-v4-crt": "^3.418.0",
    "@google-cloud/storage": "^7.1.0",
    "adm-zip": "^0.5.10",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "compare-versions": "^6.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
